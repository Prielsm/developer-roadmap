---
import ContrasteRoadmapEditor from '../../components/ContrasteRoadmap/ContrasteEditorRoadmap';
import BaseLayout from '../../layouts/BaseLayout.astro';

export const prerender = false;
const { idRoadmap } = Astro.params;

if (!idRoadmap) return null;
---

<BaseLayout
  title='Contraste Roadmaps'
  description={'Step by step guides and paths to learn different tools or technologies'}
  permalink={`/r/${idRoadmap}`}
>
  <ContrasteRoadmapEditor client:load name={idRoadmap} />
</BaseLayout>
